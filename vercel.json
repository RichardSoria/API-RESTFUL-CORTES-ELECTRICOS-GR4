{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      },
      {
        "src": "/models/user",
        "dets": "/models/user.js"
      }
      ,
      {
        "src": "/models/tools",
        "dets": "/models/tools.js"
      }
    ]
  }
  